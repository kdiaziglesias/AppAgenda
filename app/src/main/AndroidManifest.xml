        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Agenda"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Contacto"
            android:label="@string/title_activity_contacto" >
        </activity>
        <activity
            android:name=".ListaAgenda"
            android:label="@string/title_activity_lista_agenda" >
        </activity>
        <activity
            android:name=".SegundaActividad"
            android:label="@string/title_activity_segunda_actividad" >
        </activity>
        <activity
            android:name=".BorrarLista"
            android:label="@string/title_activity_borrar_lista" >
        </activity>
        <activity
            android:name=".BorraAgenda"
            android:label="@string/title_activity_borra_agenda" >
        </activity>
    </application>

</manifest>
